                opacity: 1;
                transform: scale(1) rotate(180deg);
                filter: blur(0px);
            }
            75% {
                opacity: 0.6;
                transform: scale(1.5) rotate(270deg);
                filter: blur(3px);
            }
            100% {
                opacity: 0;
                transform: scale(3) rotate(360deg);
                filter: blur(15px);
            }
        }
        
